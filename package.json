{
  "name": "ttsscoremedia-middleware",
  "version": "1.0.0",
  "description": "Middleware de integração entre TikTok Shop e VTEX, desenvolvido em NestJS + Prisma + PostgreSQL.",
  "author": "Ana Luiza <contato@ttsscoremedia.com.br>",
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:e2e": "jest --config test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "tiktok:import-categories": "ts-node scripts/import-tiktok-categories.ts",
    "catalog:sync-all": "ts-node scripts/sync-all-products.ts",
    "ok": "pm2 delete 0 && nest build && pm2 start dist/main.js"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^6.1.1",
    "@nestjs/platform-express": "^10.4.20",
    "@prisma/client": "^5.14.0",
    "axios": "^1.7.2",
    "helmet": "^7.1.0",
    "nestjs-pino": "^3.5.0",
    "pino": "^8.21.0",
    "pino-http": "^8.6.1",
    "prom-client": "^15.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.12",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.3.0",
    "prisma": "^5.14.0",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
